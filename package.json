{
  "name": "profilebuilder",
  "version": "1.0.0",
  "description": "AI-powered Resume Builder Portal",
  "private": true,
  "scripts": {
    "dev:backend": "cd backend && npm run start:dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "echo Run 'npm run dev:backend' and 'npm run dev:frontend' in separate terminals",
    "build": "cd backend && npm run build && cd ../frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "node server.js",
    "start:backend": "cross-env NODE_ENV=production node server.js",
    "start:frontend": "cd frontend && npm run start",
    "start:all": "node scripts/start-production.js",
    "prod:build": "npm run build",
    "prod:backend": "npm run build:backend && npm run start:backend",
    "prod:frontend": "npm run build:frontend && npm run start:frontend",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:populate": "playwright test tests/populate-sample-data.spec.ts",
    "test:populate:api": "playwright test tests/populate-via-api.spec.ts",
    "test:report": "playwright show-report",
    "lint": "cd backend && npm run lint && cd ../frontend && npm run lint"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "cross-env": "^7.0.3",
    "docx-parser": "^0.2.1",
    "lucide-react": "^0.556.0",
    "next": "^15.3.3",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "mongoose": "^9.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
